{
  "name": "alexa-spotify-connect",
  "version": "1.0.0",
  "description": "Control Spotify Connect devices with Alexa",
  "author": "Lawrence Thorpe",
  "dependencies": {
    "alexa-app": "^4.2.2",
    "express": "^4.15.3",
    "i18n": "^0.8.3",
