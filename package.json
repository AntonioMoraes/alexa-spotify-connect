    "request": "^2.81.0",
    "request-promise-native": "^1.0.5"
  },
  "license": "MIT",
  "main": "src/connect.js",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/i18n": "^0.8.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.3",
    "@types/node-cache": "^4.1.1",
    "@types/request-promise-native": "^1.0.15",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.0",
    "jest": "^22.1.4",
    "nock": "^9.1.6",
    "p-event": "^2.0.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
